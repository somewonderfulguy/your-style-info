{
  "name": "your-style-info",
  "version": "0.1.0",
  "private": true,
  "author": "Dmitriy Yastrebov",
  "dependencies": {
    "@tanem/react-nprogress": "^3.0.28",
    "bluebird": "^3.5.5",
    "body-scroll-lock": "^2.6.4",
    "i18next": "^19.4.5",
    "i18next-browser-languagedetector": "^4.2.0",
    "i18next-xhr-backend": "^3.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.5.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.4.1",
    "react-scroll": "^1.7.14",
    "react-spring": "^8.0.27",
    "redux": "^4.0.4",
    "redux-starter-kit": "^0.6.3",
    "resize-observer-polyfill": "^1.5.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:once": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:pre-push": "cross-env CI=true react-scripts test --coverage --env=jsdom --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^10.3.1",
    "cross-env": "^7.0.2",
    "eslint-plugin-jest": "^23.10.0",
    "husky": "^4.2.1",
    "react-test-renderer": "^16.13.1",
    "snapshot-diff": "^0.8.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js",
      "!<rootDir>/src/index.js",
      "!<rootDir>/src/api/**",
      "!<rootDir>/src/ApplicationNode/*",
      "!<rootDir>/src/assets/**",
      "!<rootDir>/src/shared/utils/**",
      "!<rootDir>/src/services/**",
      "!<rootDir>/**/useResizeObserver.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 83,
        "branches": 81,
        "functions": 74,
        "lines": 83
      }
    }
  }
}
