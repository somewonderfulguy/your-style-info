{
  "name": "your-style-info",
  "version": "0.1.0",
  "private": true,
  "author": "Dmitriy Yastrebov",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "react-scripts test",
    "test:once": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:debug": "./node_modules/.bin/react-scripts --inspect-brk test --env=jsdom --runInBand",
    "test:pre-push": "cross-env CI=true react-scripts test --coverage --env=jsdom --watchAll=false",
    "check-types": "tsc",
    "lint": "eslint --ignore-path .gitignore . --max-warnings 0 --fix",
    "validate": "pnpm run check-types && pnpm run lint",
    "postinstall": "husky install",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tanem/react-nprogress": "^3.0.30",
    "body-scroll-lock": "^4.0.0-beta.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.19.1",
    "react-router-dom": "^6.21.1",
    "react-scroll": "^1.8.3",
    "react-spring": "^8.0.27",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.2.1",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-scroll": "^1.8.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.2",
    "eslint": "^8.55.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-vitest": "^0.3.17",
    "husky": "^6.0.0",
    "intersection-observer": "^0.12.0",
    "mq-polyfill": "^1.1.8",
    "msw": "^0.34.0",
    "prettier": "^3.1.1",
    "snapshot-diff": "^0.8.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-eslint": "^1.8.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js",
      "!<rootDir>/src/index.js",
      "!<rootDir>/src/shared/utils/**",
      "!<rootDir>/src/services/**",
      "!<rootDir>/**/useResizeObserver.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 69,
        "branches": 57,
        "functions": 65,
        "lines": 69
      }
    }
  },
  "wallaby": {
    "name": "Your Style - Wallaby App"
  }
}