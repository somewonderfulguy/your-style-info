{
  "name": "your-style-info",
  "version": "0.1.0",
  "private": true,
  "author": "Dmitriy Yastrebov",
  "dependencies": {
    "@tanem/react-nprogress": "^3.0.30",
    "bluebird": "^3.5.5",
    "body-scroll-lock": "^2.6.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.4.1",
    "react-scroll": "^1.7.14",
    "react-spring": "^8.0.27",
    "resize-observer-polyfill": "^1.5.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:once": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:debug": "./node_modules/.bin/react-scripts --inspect-brk test --env=jsdom --runInBand",
    "test:pre-push": "cross-env CI=true react-scripts test --coverage --env=jsdom --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^10.3.1",
    "cross-env": "^7.0.2",
    "eslint-plugin-jest": "^23.10.0",
    "husky": "^4.2.1",
    "intersection-observer": "^0.11.0",
    "react-test-renderer": "^16.13.1",
    "snapshot-diff": "^0.8.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js",
      "!<rootDir>/src/index.js",
      "!<rootDir>/src/api/**",
      "!<rootDir>/src/ApplicationNode/*",
      "!<rootDir>/src/assets/**",
      "!<rootDir>/src/shared/utils/**",
      "!<rootDir>/src/services/**",
      "!<rootDir>/**/useResizeObserver.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 76,
        "branches": 65,
        "functions": 65,
        "lines": 76
      }
    }
  }
}
