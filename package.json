  "dependencies": {
    "bluebird": "^3.5.5",
    "body-scroll-lock": "^2.6.4",
    "i18next": "^17.0.14",
    "i18next-browser-languagedetector": "^3.0.3",
    "i18next-xhr-backend": "^3.1.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.12.4",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-scroll": "^1.7.14",
    "react-spring": "^8.0.27",
    "redux": "^4.0.4",
    "redux-starter-kit": "^0.6.3",
