{
  "name": "your-style-info",
  "version": "0.1.0",
  "private": true,
  "author": "Dmitriy Yastrebov",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:once": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:debug": "./node_modules/.bin/react-scripts --inspect-brk test --env=jsdom --runInBand",
    "test:pre-push": "cross-env CI=true react-scripts test --coverage --env=jsdom --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint --ignore-path .gitignore .",
    "postinstall": "husky install",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tanem/react-nprogress": "^3.0.30",
    "bluebird": "^3.5.5",
    "body-scroll-lock": "^3.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.17.2",
    "react-router-dom": "^5.0.1",
    "react-scripts": "4.0.3",
    "react-scroll": "^1.7.14",
    "react-spring": "^8.0.27",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.0",
    "@testing-library/user-event": "^12.8.3",
    "cross-env": "^7.0.2",
    "eslint-plugin-jest": "^24.2.0",
    "husky": "^6.0.0",
    "intersection-observer": "^0.12.0",
    "mq-polyfill": "^1.1.8",
    "snapshot-diff": "^0.8.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js",
      "!<rootDir>/src/index.js",
      "!<rootDir>/src/api/**",
      "!<rootDir>/src/ApplicationNode/*",
      "!<rootDir>/src/assets/**",
      "!<rootDir>/src/shared/utils/**",
      "!<rootDir>/src/services/**",
      "!<rootDir>/**/useResizeObserver.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 69,
        "branches": 57,
        "functions": 65,
        "lines": 69
      }
    }
  },
  "wallaby": {
    "name": "Your Style - Wallaby App"
  }
}
